<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="height">400</property>
    <property name="width">300</property>
    <property name="title" translatable="yes">Calculator</property>
    <child>
      <object class="ttk.Frame" id="main_frame">
        <property name="padding">10</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
        </layout>
        <child>
          <object class="ttk.Entry" id="display">
            <property name="font">{Arial} 18</property>
            <property name="justify">right</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="pady">0 10</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="button_frame">
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
            </layout>
            <child>
              <object class="ttk.Button" id="btn_7">
                <property name="text" translatable="yes">7</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_8">
                <property name="text" translatable="yes">8</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_9">
                <property name="text" translatable="yes">9</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_div">
                <property name="text" translatable="yes">/</property>
                <layout manager="grid">
                  <property name="column">3</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_4">
                <property name="text" translatable="yes">4</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_5">
                <property name="text" translatable="yes">5</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_6">
                <property name="text" translatable="yes">6</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_mul">
                <property name="text" translatable="yes">*</property>
                <layout manager="grid">
                  <property name="column">3</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_1">
                <property name="text" translatable="yes">1</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">2</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_2">
                <property name="text" translatable="yes">2</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="row">2</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_3">
                <property name="text" translatable="yes">3</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">2</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_sub">
                <property name="text" translatable="yes">-</property>
                <layout manager="grid">
                  <property name="column">3</property>
                  <property name="row">2</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_0">
                <property name="text" translatable="yes">0</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">3</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_dot">
                <property name="text" translatable="yes">.</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="row">3</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_equals">
                <property name="text" translatable="yes">=</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">3</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_add">
                <property name="text" translatable="yes">+</property>
                <layout manager="grid">
                  <property name="column">3</property>
                  <property name="row">3</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_clear">
                <property name="text" translatable="yes">C</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="row">4</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_back">
                <property name="text" translatable="yes">Back</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="columnspan">2</property>
                  <property name="row">4</property>
                  <property name="sticky">nsew</property>
                  <property name="padx">2</property>
                  <property name="pady">2</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
